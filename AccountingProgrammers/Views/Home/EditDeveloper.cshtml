@model AccountingProgrammers.Models.Developer

@{
    ViewData["Title"] = "EditDeveloper";
}

<h2>EditDeveloper</h2>

<h4>Developer</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="EditDeveloper">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="DeveloperId" />
            <div class="form-group">
                <label asp-for="DeveloperName" class="control-label"></label>
                <input asp-for="DeveloperName" class="form-control" />
                <span asp-validation-for="DeveloperName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="DeveloperList">Back to List</a>
</div>

