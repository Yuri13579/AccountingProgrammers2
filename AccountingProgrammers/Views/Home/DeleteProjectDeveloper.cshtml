@model AccountingProgrammers.Models.ProjectDeveloper

@{
    ViewData["Title"] = "DeleteProjectDeveloper";
}

<h2>DeleteProjectDeveloper</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>ProjectDeveloper</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Project.ProjectId)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Project.ProjectName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Developer)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Developer.DeveloperName)
        </dd>
    </dl>
    
    <form asp-action="DeleteProjectDeveloper">
        <input type="hidden" asp-for="ProjectId" />
        <input type="hidden" asp-for="DeveloperId" />
        <input type="submit" value="Delete" class="btn btn-default" /> |
        <a asp-action="ProjectDeveloperList">ProjectDeveloperList</a>
    </form>
</div>
